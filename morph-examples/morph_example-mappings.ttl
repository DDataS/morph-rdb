<#TriplesMap19>
	a rr:TriplesMap;

	rr:logicalTable [ 
		rr:sqlQuery "SELECT DISTINCT t_77150.\"Id\" FROM (\"sport\" AS t_77150 JOIN \"student\" AS t_19335 ON ((t_77150.\"Id\"=t_19335.\"Sport\")))" 
	];

	rr:subjectMap [
		rr:template "http://example.com/sport/{Id}";
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
		rr:predicate <http://example.com/sportHasstudent>;
		rr:objectMap [ rr:template "http://example.com/student/{Id}" ];
	].

